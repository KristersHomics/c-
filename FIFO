#include <iostream>     
#include <stack> 
using namespace std;     

int main ()
{
  stack<int> queue;
  int myint;

  cout << "Ievadi skaitlus(0 beidz ievadi):\n";

  do {
    cin >> myint;
    queue.push (myint);
  } while (myint);
  
 
  cout << "Rinda: ";

   while (!queue.empty()) {
        cout << queue.top() << "\n";
        queue.pop();
    }
    

  return 0;
}
